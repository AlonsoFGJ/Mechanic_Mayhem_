{
    "version": 2,
    "routes": [
      {
        "src": "/media/(.*)",
        "dest": "http://127.0.0.1:8000/media/$1",
        "headers": {
          "Cache-Control": "s-maxage=31536000, stale-while-revalidate"
        }
      },
      {
        "src": "/(.*)",
        "dest": "/Pagina/wsgi.py"
      }
    ]
  }
